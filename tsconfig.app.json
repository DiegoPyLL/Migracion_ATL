{
  "compilerOptions": {
    "types": ["vite/client", "vitest", "node"],
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "baseUrl": "./src"
  },
  "include": ["src"]
}
